{
  "name": "memesite",
  "version": "1.0.0",
  "description": "Practice grid, flex and layout",
  "default": "index.html",
  "scripts": {
    "compile:scss": "node-sass src/sass/main.scss src/css/style.css",
    "watch:scss": "node-sass src/sass/main.scss src/css/style.css -w",
    "start": "parcel index.html",
    "build": "parcel build index.html --dist-dir ./dist"
  },
